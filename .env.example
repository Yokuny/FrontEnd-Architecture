# API Configuration
VITE_URI_BASE=http://localhost:3001/api/v1
VITE_URI_SOCKET=http://localhost:3005
VITE_URI_SUPPORT=http://localhost:3010/api/v1
VITE_URI_TIMEOUT=100000
VITE_API_URL=http://localhost:3000

# ReCAPTCHA
VITE_RECAPTCHA_SITE_KEY=token_google_recaptcha

# File Upload
VITE_MAX_SIZE_FILE_BYTES=26214400

# Map
VITE_MAP_TILER=token

# SSO Configuration
VITE_SSO_TOKEN="secret_sso"
VITE_SSO_KEY="secret_sso"
VITE_SECRET_KEY_REQUEST=secret

# Azure AD Configuration (for SSO)
VITE_AZURE_CLIENT_ID=your_azure_client_id
VITE_AZURE_TENANT_ID=your_azure_tenant_id
VITE_AZURE_REDIRECT_URI=/
VITE_AZURE_POST_LOGOUT_REDIRECT_URI=/

# Build Configuration (não precisa de VITE_ prefix pois não é exposto ao cliente)
GENERATE_SOURCEMAP=false
