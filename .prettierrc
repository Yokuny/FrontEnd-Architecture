{
    "semi": true,
    "useTabs": true,
    "tabWidth": 2,
    "printWidth": 180,
    "singleQuote": true,
    "trailingComma": "es5",
    "singleAttributePerLine": false,
    "htmlWhitespaceSensitivity": "ignore",
    "bracketSameLine": true,
    "plugins": [
        "prettier-plugin-organize-imports",
        "prettier-plugin-tailwindcss"
    ]
}