{
  "name": "front-end-architecture",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "start": "vite",
    "lint": "biome check --write .",
    "format": "biome check --write --unsafe && biome format --write .",
    "check": "tsc --noEmit",
    "routes": "npx tsx src/config/extractRoutes.ts && pnpm run format",
    "routes:ai": "npx tsx src/config/extractRoutesMetadata.ts",
    "i18n": "python3 src/config/translations/i18n_pipeline.py",
    "i18n:clean": "python3 src/config/translations/i18n_remove_unused.py",
    "i18n:sort": "python3 src/config/translations/i18n_sort.py",
    "i18n:dedupe": "python3 src/config/translations/i18n_deduplicate.py",
    "i18n:check": "python3 src/config/translations/i18n_check_missing.py",
    "i18n:validate": "python3 src/config/translations/i18n_validate.py"
  },
